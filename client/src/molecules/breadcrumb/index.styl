.breadcrumb
  display flex
  align-items center
  font-size 14px
  color #666

  .breadcrumb-list
    list-style none
    margin 0
    padding 0
    display flex
    align-items center

  .breadcrumb-item
    display flex
    align-items center

  .breadcrumb-label
    color #666
    cursor pointer
    font-weight 500
    &:hover
      color #007bff

  .breadcrumb-separator
    margin 0 3px
    color #ccc
