{
    "name": "nebula-note",
    "private": true,
    "version": "1.0.0",
    "description": "It refers to a tool to optimize efficiency ",
    "main": "pro/main.js",
    "author": "xu.huang",
    "license": "MIT",
    "engines": {
        "node": ">=16",
        "yarn": ">=1.22"
    },
    "scripts": {
        "dev": "yarn client:dev & yarn serve & yarn client:storyBook",
        "client:storyBook": "yarn workspace  @nebula-note/client storybook",
        "client:dev": "yarn workspace  @nebula-note/client dev",
        "client:cypress:run": "yarn workspace @nebula-note/client cyperss:run:component",
        "serve": "node ./service/index.js",
        "build:client": "tsc && vite build",
        "build:client:watch": "tsc && vite build -w",
        "build:pro": "yarn build:client && electron-builder --mac",
        "start": "node service/index.js",
        "start:web": "yarn build:client && yarn start",
        "prettier:fix": "prettier --write ."
    },
    "dependencies": {
        "@koa/router": "^13.1.0",
        "benchmark": "^2.1.4",
        "ejs": "^3.1.10",
        "eslint": "^9.16.0",
        "koa": "^2.15.3",
        "koa-bodyparser": "^4.4.1",
        "lodash": "^4.17.21",
        "node-fetch": "^3.3.2",
        "rimraf": "5.0.10",
        "simple-git": "^3.20.0"
    },
    "workspaces": [
        "./nebula-client",
        "./nebula-ui",
        "./nebula-shell"
    ],
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "^5.0.1",
        "cypress": "^13.17.0",
        "prettier": "^3.4.2",
        "prettier-plugin-svelte": "^3.3.2",
        "svelte": "^5.7.1"
    }
}
